{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "FLASK_ENV": "production",
        "MAX_FETCH_LIMIT": "300",
        "REQUEST_TIMEOUT": "20",
        "MAX_RETRIES": "3",
        "CACHE_EXPIRY": "86400",
        "YOUTUBE_USER_AGENT": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36"
    }
}